/**app.wxss**/
button {
  padding: 0;
  outline: none;
  border: none;
  font-weight: normal;
}
button:focus {
  outline: 0;
}
button:after {
  border: none;
}
input {
  padding: 0;
  outline: none;
  border: none;
  list-style: none;
}
page {
  background: #fff;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.test-test-test-test {
  animation: ani-roll 1s infinite linear both;
  border-top: 2rpx solid #f2f2f2;
  border-bottom: 20rpx solid rgb(246, 246, 246);
  transition: transform 0.15s ease-in-out;
  background: 0 center no-repeat;
}
