/**app.wxss**/

.container {
  width: 100%;
  background: #fff;
}

button {
  background: initial;
}

button:focus {
  outline: 0;
}

button::after {
  border: none;
}

input {
  outline: none;
  border: none;
  list-style: none;
}

page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
